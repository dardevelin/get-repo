#!/bin/bash

# Copy this file to sign-config.sh and update with your credentials
# DO NOT commit sign-config.sh to git!

# Your Developer ID certificate name
# Find it with: security find-identity -v -p codesigning
export DEVELOPER_ID="Developer ID Application: Your Name (TEAM_ID)"

# Your Apple ID email
export APPLE_ID="your-apple-id@example.com"

# App-specific password for notarization
# Create at: https://appleid.apple.com/account/manage
export APP_PASSWORD="xxxx-xxxx-xxxx-xxxx"

# Bundle ID for your app
export BUNDLE_ID="com.dbrasdasilva.get-repo"

# Team ID (found in your Apple Developer account)
export TEAM_ID="XXXXXXXXXX"